{
	"name": "stock-scraper",
	"version": "1.0.0",
	"description": "Api for scraping value of stock from Google Finance or Yahoo Finance",
	"main": "app.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"start": "fastify start -l info app.js",
		"dev": "fastify start -w -l info -P app.js",
		"test": "tap \"test/**/*.test.js\" --coverage",
		"build": "node ./scripts/build.js",
		"postbuild": "prettier --write .",
		"lint": "eslint --fix --ext .js {routes,scripts,test,utils}/*.js {routes,test}/**/*.js"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@gitlab.com/SatvikG7/stock-scraper.git"
	},
	"homepage": "https://satvikg7.gitlab.io/stock-scraper/",
	"bugs": {
		"url": "https://gitlab.com/SatvikG7/stock-scraper/issues"
	},
	"keywords": [
		"node",
		"fastify",
		"jest",
		"eslint",
		"stocks",
		"api"
	],
	"author": "Satvik Gaikwad",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.24.0",
		"cheerio": "^1.0.0-rc.10",
		"dotenv": "^10.0.0",
		"fastify": "^3.0.0",
		"fastify-autoload": "^3.3.1",
		"fastify-cli": "^2.14.0",
		"fastify-plugin": "^3.0.0",
		"fastify-static": "^4.5.0"
	},
	"devDependencies": {
		"eslint": "8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"highlight.js": "^11.3.1",
		"marked": "^4.0.6",
		"prettier": "2.5.0",
		"tap": "^15.0.9"
	}
}
