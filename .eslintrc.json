{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		],
		"no-unused-vars": "warn",
		"no-console": "warn",
		"no-debugger": "warn",
		"no-alert": "warn",
		"no-empty": "warn",
		"no-extra-semi": "warn",
		"func-names": "off",
		"object-shorthand": "off"
	}
}
